<div id="user-info">
  <%= @user.name %>
  <a href="/users/<%= @user.id %>"></a>
</div>
<h1>Welcome to H2H Quizzes</h1>
<ul>
  <% @quizzes.each do |quiz| %>
    <li class="quiz-list">
      <%= link_to quiz.name, quiz_question_path(quiz, quiz.questions.first) %>
    </li>
  <% end  %>
</ul>
<p>Find me in app/views/quiz/index.html.erb</p>
